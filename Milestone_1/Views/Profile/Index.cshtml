@model Microsoft.AspNetCore.Identity.IdentityUser

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["Title"] = "Profile";

}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Twitter App</title>
</head>
<body>
 <h1>Hello world</h1>

    <div class="row">

        <div class="col-md-3">
            <ul class="nav nav-pills nav-stacked">
                <li class="active">
                    <a asp-controller="Profile" asp-action="Index">Profile</a>
                            </li>
                <li><a asp-controller="Profile" asp-action="Tweets">Tweets</a></li>
                <li><a asp-controller="Profile" asp-action="Comments">Comments</a></li>
                <li><a asp-controller="Profile" asp-action="Followers">Comments</a></li>
            </ul>
        </div>

        <div class="col-md-9">
            Here some part of Profile Page
            @{
                if (TempData.Peek("UserName") != null)
                {
        <h3 class="text-success">@TempData.Peek("UserName")</h3>

        } else
                {
        <h3 class="text-danger"> Bad request!</h3>
                }

                if (TempData.Peek("IdentityUserSes") != null) {
                            <h3 class="text-success">@TempData.Peek("IdentityUserSes")</h3>

                }

}



        </div>

    </div>


</body>
</html>